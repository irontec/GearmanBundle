{
    "name": "grimbode/gearman-bundle",
    "replace": {
        "mmoreramerino/gearman-bundle": "dev-master"
    },
    "description": "Adds gearman support to your Symfony3 project",
    "keywords": ["gearman"],
    "homepage": "https://github.com/grimbode/GearmanBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/grimbode/GearmanBundle"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-gearman": "*",

        "doctrine/doctrine-cache-bundle": "~1.0",
        "doctrine/common": "~2.2",

        "symfony/dependency-injection": "^3.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/http-kernel": "^3.0",
        "symfony/console": "^3.0",
        "symfony/config": "^3.0",
        "symfony/finder": "^3.0",
        "symfony/options-resolver": "^3.0"
    },
    "target-dir": "Grimbode/GearmanBundle",
    "autoload": {
        "psr-0": { "Grimbode\\GearmanBundle": "" }
    },
    "minimum-stability": "stable", 
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
